<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.money">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".Transaction.EditTransactionActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".Home.ChartActivity" />
        <activity android:name=".Home.TransactionCategoryActivity" />
        <activity android:name=".Transaction.AddIncomeActivity" />
        <activity android:name=".Transaction.DetailTransactionActivity" />
        <activity
            android:name=".Home.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Transaction.AddExpenseActivity" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>